<T3DataStructure>
<meta>            
<langDisable>1</langDisable>
</meta>   
  <sheets>
    <sDEF>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.sheet_text</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <text>
            <TCEforms>
              <label>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.text</label>
              <config>
                <type>text</type>
                <cols>30</cols>
                <rows>5</rows>
              </config>
              <defaultExtras>richtext[formatblock|bold|italic|underline|left|center|right|orderedlist|unorderedlist|outdent|indent|link|image|chMode]:rte_transform[mode=css]</defaultExtras>
            </TCEforms>
          </text>   
        </el>
      </ROOT>
    </sDEF>
    <popeye>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.sheet_popeye</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <images>
            <TCEforms>
              <label>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.images</label>
                <config type="array">
                 <type>group</type>   
                 <internal_type>file</internal_type>
                 <allowed>jpg,gif,png</allowed>                   
                 <max_size>2000</max_size>                 
                 <uploadfolder>uploads/pics</uploadfolder>
                 <show_thumbs>1</show_thumbs>
                 <size>3</size>
                 <autoSizeMax>30</autoSizeMax>
                 <minitems>0</minitems>
                 <maxitems>30</maxitems>
                </config> 
            </TCEforms>
          </images> 
          <caption>
            <TCEforms>
              <label>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.captions</label>
                <config>
                 <type>text</type>   
                 <cols>27</cols>
                 <rows>4</rows>
                </config> 
            </TCEforms>
          </caption>   
        </el>
      </ROOT>
    </popeye>
    <options>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.sheet_options</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>             
          <width>
            <TCEforms>
              <label>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.width</label>
              <config>
                <type>input</type>
                <size>5</size>
              </config>
            </TCEforms>
          </width>
          <noscript>
            <TCEforms>
              <label>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.noscript</label>
              <config>
                <type>input</type>
                <size>25</size>
              </config>
            </TCEforms>
          </noscript>
          <ofLabel>
            <TCEforms>
              <label>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.ofLabel</label>
              <config>
                <type>input</type>
                <size>5</size>
              </config>
            </TCEforms>
          </ofLabel>    
          <align>
            <TCEforms>
              <label>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.float</label>
              <config>
                <type>select</type>
                <items type="array">
                  <numIndex index="0" type="array">
                    <numIndex index="0">LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.left</numIndex>
                    <numIndex index="1">1</numIndex>
                  </numIndex>
                  <numIndex index="1" type="array">
                    <numIndex index="0">LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.right</numIndex>
                    <numIndex index="1">2</numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </align>   
          <direction>
            <TCEforms>
              <label>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.direction</label>
              <config>
                <type>select</type>
                <items type="array">
                  <numIndex index="0" type="array">
                    <numIndex index="0">LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.left</numIndex>
                    <numIndex index="1">right</numIndex>
                  </numIndex>
                  <numIndex index="1" type="array">
                    <numIndex index="0">LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.right</numIndex>
                    <numIndex index="1">left</numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </direction>   
          <duration>
            <TCEforms>
              <label>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.duration</label>
              <config>
                <type>input</type>
                <size>5</size>
              </config>
            </TCEforms>
          </duration>
          <opacity>
            <TCEforms>
              <label>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.opacity</label>
              <config>
                <type>select</type>
                <items type="array">
                  <numIndex index="0" type="array">
                    <numIndex index="0">1.0</numIndex>
                    <numIndex index="1">1.0</numIndex>
                  </numIndex>
                  <numIndex index="1" type="array">
                    <numIndex index="0">0.8</numIndex>
                    <numIndex index="1">0.8</numIndex>
                  </numIndex>
                  <numIndex index="2" type="array">
                    <numIndex index="0">0.6</numIndex>
                    <numIndex index="1">0.6</numIndex>
                  </numIndex>
                  <numIndex index="3" type="array">
                    <numIndex index="0">0.4</numIndex>
                    <numIndex index="1">0.4</numIndex>
                  </numIndex>
                  <numIndex index="4" type="array">
                    <numIndex index="0">0.2</numIndex>
                    <numIndex index="1">0.2</numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </opacity> 
          <countpos>
            <TCEforms>
              <label>LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.countpos</label>
              <config>
                <type>select</type>
                <items type="array">
                  <numIndex index="0" type="array">
                    <numIndex index="0">Overlay</numIndex>
                    <numIndex index="1">overlay</numIndex>
                  </numIndex>
                  <numIndex index="1" type="array">
                    <numIndex index="0">Caption</numIndex>
                    <numIndex index="1">caption</numIndex>
                  </numIndex>
                  <numIndex index="2" type="array">
                    <numIndex index="0">LLL:EXT:rzpopeye/locallang_tca.xml:rzpopeye.pi_flexform.none</numIndex>
                    <numIndex index="1">false</numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </countpos>  
        </el>
      </ROOT>
    </options>
  </sheets>
</T3DataStructure>

